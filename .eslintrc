{
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "comma-dangle": [
      2,
      "always-multiline"
    ],
    "function-paren-newline": [
      "warn",
      "consistent"
    ],
    "no-cond-assign": [
      2,
      "always"
    ],
    "no-constant-condition": 1,
    "no-control-regex": 1,
    "no-debugger": 1,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 1,
    "no-empty-character-class": 1,
    "no-empty": 1,
    "no-ex-assign": 1,
    "no-extra-parens": [
      1,
      "all"
    ],
    "no-extra-semi": 1,
    "no-func-assign": 1,
    "no-inner-declarations": [
      1,
      "both"
    ],
    "no-irregular-whitespace": 1,
    "no-negated-in-lhs": 1,
    "no-obj-calls": 1,
    "no-regex-spaces": 1,
    "no-sparse-arrays": 1,
    "no-unreachable": 1,
    "use-isnan": 1,
    "valid-jsdoc": 1,
    "valid-typeof": 2,
    "no-unexpected-multiline": 0,
    "accessor-pairs": [
      1,
      {
        "getWithoutSet": true,
        "setWithoutGet": true
      }
    ],
    "consistent-return": 2,
    "default-case": 1,
    "dot-notation": [
      1,
      {
        "allowKeywords": false
      }
    ],
    "dot-location": [
      2,
      "property"
    ],
    "eqeqeq": [
      2,
      "smart"
    ],
    "guard-for-in": 2,
    "no-alert": 1,
    "no-caller": 2,
    "no-else-return": 1,
    "no-extend-native": 1,
    "no-extra-bind": 1,
    "no-fallthrough": 1,
    "no-floating-decimal": 1,
    "no-implicit-coercion": 1,
    "no-implied-eval": 1,
    "no-invalid-this": 1,
    "no-labels": 1,
    "no-loop-func": 1,
    "no-multi-spaces": 1,
    "no-native-reassign": 2,
    "no-new-func": 1,
    "no-new-wrappers": 1,
    "no-new": 1,
    "no-param-reassign": [
      1,
      {
        "props": false
      }
    ],
    "no-redeclare": 1,
    "no-return-assign": [
      1,
      "always"
    ],
    "no-script-url": 1,
    "no-self-compare": 1,
    "no-sequences": 2,
    "no-throw-literal": 1,
    "no-useless-call": 1,
    "no-void": 1,
    "no-with": 2,
    "wrap-iife": [
      2,
      "any"
    ],
    "yoda": [
      1,
      "always",
      {}
    ],
    "no-catch-shadow": 1,
    "no-delete-var": 2,
    "no-shadow": [
      1,
      {
        "hoist": "all"
      }
    ],
    "no-unused-vars": 1,
    "no-use-before-define": 1,
    "brace-style": [
      1,
      "1tbs",
      {}
    ],
    "camelcase": [
      1,
      {
        "properties": "always"
      }
    ],
    "comma-spacing": [
      1,
      {
        "after": true
      }
    ],
    "comma-style": 1,
    "computed-property-spacing": [
      1,
      "never"
    ],
    "eol-last": 2,
    "func-style": [
      1,
      "expression"
    ],
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "linebreak-style": [
      1,
      "unix"
    ],
    "new-cap": 1,
    "new-parens": 1,
    "no-array-constructor": 1,
    "no-mixed-spaces-and-tabs": 2,
    "no-new-object": 1,
    "no-spaced-func": 1,
    "no-trailing-spaces": 1,
    "no-unneeded-ternary": 1,
    "operator-assignment": [
      1,
      "always"
    ],
    "operator-linebreak": [
      1,
      "before"
    ],
    "semi": [
      2,
      "never"
    ],
    "keyword-spacing": [
      1,
      {
        "before": true,
        "after": true
      }
    ],
    "space-before-blocks": [
      1,
      "always"
    ],
    "space-infix-ops": 1,
    "space-in-parens": "off",
    "arrow-spacing": [
      1,
      {
        "before": true,
        "after": true
      }
    ],
    "constructor-super": 1,
    "no-class-assign": 1,
    "no-const-assign": 2,
    "no-this-before-super": 1,
    "no-var": 2,
    "object-shorthand": [
      1,
      "properties"
    ],
    "object-curly-spacing": [
      1,
      "never"
    ],
    "prefer-const": 1,
    "prefer-spread": 1,
    "quotes": [
      "warn",
      "single"
    ],
    "jsx-quotes": [
      "warn",
      "prefer-double"
    ],
    "react/boolean-prop-naming": 2,
    "react/default-props-match-prop-types": 1,
    "react/destructuring-assignment": 2,
    "react/display-name": 2,
    "react/no-children-prop": 1,
    "react/no-multi-comp": 2,
    "react/no-typos": 1,
    "react/no-unused-prop-types": 2,
    "react/no-unused-state": 2,
    "react/prefer-es6-class": [
      2,
      "always"
    ],
    "react/react-in-jsx-scope": 1,
    "react/require-render-return": 1,
    "react/self-closing-comp": 1,
    "react/jsx-boolean-value": [
      2,
      "never"
    ],
    "react/jsx-child-element-spacing": 2,
    "react/jsx-equals-spacing": 2,
    "react/jsx-indent": [
      2,
      2
    ],
    "react/jsx-key": 1,
    "react/jsx-no-duplicate-props": 2,
    "react/jsx-no-undef": 2,
    "react/jsx-pascal-case": 2,
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2
  }
}